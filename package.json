{
  "name": "typed-bi-directional-map",
  "version": "1.0.2",
  "description":
    "Provides a Bi-Directional Map class that conforms to Map interface, but also allows one to lookup key from value.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test": "jest",
    "prepublish": "tsc && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ThomasRooney/typed-bi-directional-map.git"
  },
  "keywords": [
    "map",
    "bidirectional map",
    "typed",
    "typesafe",
    "typescript",
    "bi directional map",
    "two way map"
  ],
  "author": "Thomas Rooney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThomasRooney/typed-bi-directional-map/issues"
  },
  "homepage": "https://github.com/ThomasRooney/typed-bi-directional-map#readme",
  "files": ["lib"],
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "prettier": "1.12.1",
    "ts-jest": "^22.4.6",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.12.0",
    "tslint-eslint-rules": "5.1.0",
    "typescript": "2.8.3"
  },
  "jest": {
    "roots": ["<rootDir>/src"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }
}
